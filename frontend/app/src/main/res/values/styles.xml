        <item name="fontFamily">@font/medium</item>
    </style>

    <style name="button_primary_style" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textColor">@color/btn_text_color_primary</item>
        <item name="android:textSize">16sp</item>
        <item name="fontFamily">@font/semi_bold</item>
        <item name="strokeWidth">0dp</item>
        <item name="backgroundTint">@color/color_primary</item>
        <item name="cornerRadius">52dp</item>
        <item name="android:layout_height">60dp</item>
