    </style>

    <style name="textInputEditText.OutlinedBox" parent="ThemeOverlay.Material3.TextInputEditText.OutlinedBox">
        <item name="android:textColor">@color/edittext_color</item>
        <item name="android:textSize">16sp</item>
        <item name="fontFamily">@font/regular</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:layout_marginBottom">15dp</item>
        <item name="boxBackgroundColor">@color/color_transparent</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="boxCornerRadiusBottomEnd">16dp</item>
        <item name="boxCornerRadiusBottomStart">16dp</item>
        <item name="boxCornerRadiusTopEnd">16dp</item>
        <item name="boxCornerRadiusTopStart">16dp</item>
        <item name="android:gravity">center</item>
        <item name="hintEnabled">false</item>
        <item name="boxStrokeColor">@color/stroke_color</item>
        <item name="startIconTint">@color/color_primary</item>
        <item name="strokeWidth">0.5dp</item>
        <item name="iconTint" tools:targetApi="o">@color/white</item>

    </style>

    <style name="TransparentDialog" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@drawable/dialog_background</item>
        <item name="marginLeftSystemWindowInsets">true</item>
        <item name="marginRightSystemWindowInsets">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:windowIsFloating">true</item>
<!--        <item name="android:layout_marginEnd">20dp</item>-->
<!--        <item name="android:layout_marginStart">20dp</item>-->
        <item name="android:backgroundDimEnabled">false</item>
    </style>

